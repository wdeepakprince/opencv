{
  "name": "OpenCV Dev Container",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "settings": {
    "python.pythonPath": "/usr/local/bin/python"
  },
  "extensions": [
    "ms-python.python",
    "ms-toolsai.jupyter"
  ],
  "forwardPorts": [],
  "postCreateCommand": "pip install -r requirements.txt",
  "remoteUser": "vscode"
}
